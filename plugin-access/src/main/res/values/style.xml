<?xml version="1.0" encoding="utf-8"?>
<resources
        xmlns:tools="http://schemas.android.com/tools"
        tools:ignore="ResourceName"
        >
    <style name="AppTheme" parent="android:Theme.Light" />
    <style name="home_activity_style" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <declare-styleable name="SlidingMenu" tools:ignore="ResourceName">
        <attr name="mode">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr name="viewAbove" format="reference"/>
        <attr name="viewBehind" format="reference"/>
        <attr name="behindOffset" format="dimension"/>
        <attr name="behindWidth" format="dimension"/>
        <attr name="behindScrollScale" format="float"/>
        <attr name="touchModeAbove">
            <enum name="margin" value="0"/>
            <enum name="fullscreen" value="1"/>
            <enum name="none" value="2"/>
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0"/>
            <enum name="fullscreen" value="1"/>
            <enum name="none" value="2"/>
        </attr>
        <attr name="shadowDrawable" format="reference"/>
        <attr name="shadowWidth" format="dimension"/>
        <attr name="fadeEnabled" format="boolean"/>
        <attr name="fadeDegree" format="float"/>
        <attr name="selectorEnabled" format="boolean"/>
        <attr name="selectorDrawable" format="reference"/>
    </declare-styleable>

    <style name="customergrouptablayou" tools:ignore="ResourceName">
        <item name="tabBackground">@color/white</item>
        <item name="tabIndicatorColor">@color/mian_color</item>
        <item name="tabIndicatorHeight">2dp</item>
        <item name="tabSelectedTextColor">@color/mian_color</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/text_color</item>
    </style>

    <declare-styleable name="CircleImageView" tools:ignore="ResourceName">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundRectImageVIew">
        <attr name="round_radius" format="dimension"/>
    </declare-styleable>


</resources>
